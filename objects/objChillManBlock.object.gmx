if !global.frozen &amp;&amp; !dead
{
    if instance_exists(target)
    {
        if (place_meeting(x,y-global.GravityDir,target) &amp;&amp; !CheckGround &amp;&amp; target.ground)
        or (place_meeting(x,y-target.yspeed*2,objSakugarne))
        {
            if image_index = 1
            {
                endit = 1;
            }
            event_user(10);
        }
        CheckGround = target.ground
    }
    
    if instance_exists(prtEnemy)
    {
        //with prtEnemy
        NearestEnemy = instance_nearest(x+16,y,prtEnemy)
        with NearestEnemy {
            with other 
            {
                if (place_meeting(x,y-1,NearestEnemy) &amp;&amp; !CheckGroundE &amp;&amp; NearestEnemy.yspeed &lt;= 0)
                {
                    if image_index = 1
                    {
                        endit = 1;
                    }
                    event_user(10);
                }
                if NearestEnemy.yspeed &gt; 0 {
                    CheckGroundE = false;
                }
                else {
                    CheckGroundE = true;
                }
            }
        }
    }
    
    if endit
    {
        endit += 1;
        event_user(10);
    }
}

//Handle being solid
instance_activate_object(mySolid);
if instance_exists(mySolid)
{
    if Solid &amp;&amp; !dead
    {
        mySolid.x = x;
        mySolid.y = y;
    }
    else
    {
        with mySolid {instance_destroy();}
    }
}
else if Solid &amp;&amp; !dead
{
    mySolid = instance_create(x,y,objSolidIndependent)
    mySolid.sprite_index = sprite_index;
}

if dead
{
    if room == rmMistyLake_main_notencore or room == lvlSwissHotel_Pyro  or room == rmGravitron_MJ //Notencore explitly requested different behaviour for the misty lake level.
        exit;
    image_index = 0;
    mySolid=noone;
    healthpoints = 1;
    endit = 0;
}
