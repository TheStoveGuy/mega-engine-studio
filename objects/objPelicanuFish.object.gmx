    if (ground){
        yspeed = -1.2;
        xspeed = random_range(-1.4, 1.4);
    }
    
    //ded temer
    killTimer += 1;
    if (killTimer &gt;= 100){
        instance_create(x, y, objExplosion);
        instance_destroy();
    }
    
    //sliiiiide to the left, sliiiiide to the right
    x += xspeed;
    y += yspeed;
}

