            <string>if (global.frozen) {exit;}

if (ricochet) {exit;}
if (canhit = -1) {exit;}
with other
{
    if (!canHit) {exit;}
    if (dead) {exit;}
}

guardcancel = 0;
with other {event_user(11);}
if other.wpndmg[wpnID] = 0 {guardcancel = max(1,guardcancel);}
if guardcancel = 2 {exit;} //If being outright cancelled
if guardcancel = 1 //If enemy is guarding
{
    if penetrate = 0 { //Reflect off
        if ricochet = 0 {event_user(10);} exit;
    }
    if penetrate = 1 {exit;} //Not getting reflected (no damage)
    //Set penetrate to 2 to completely bypass shields
}

if (!other.canHit) {exit;}

//If above condition are met
if canhit = 0
{
    canhit = 8;
    damage = other.wpndmg[wpnID];
    event_user(11); //Effect of the weapon
    if damage != 0 { //If damage is being dealt
        if damage &gt; 0 and other.canflash {playSFX(sfxEnemyHit);}
        var subhealth;
        //Get health
        if !other.boss {subhealth = other.healthpoints;} //Regular health
        else {subhealth = global.bossHealth[other.healthIndex];} //Boss health
        //Handle health
        subhealth -= damage;
        //Set health
        if !other.boss {other.healthpoints = subhealth;} //Regular health
        else {global.bossHealth[other.healthIndex] = subhealth;} //Boss health
        if subhealth &lt;= 0 {with other {event_user(10);}}
        if pierces = 1 { //If can pierce
            if subhealth &lt;= 0 {canhit = 0;}
            else {instance_destroy();}
        }
        with other {
            if global.damagepopup { //Damagepopup
                var damagepopup;
                damagepopup = instance_create(bbox_get_xcenter(),bbox_top+4,objDamagePopup);
                damagepopup.damage = other.damage;
