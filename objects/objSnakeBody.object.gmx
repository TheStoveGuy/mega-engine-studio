        yspeed = shakeSpeed*dir;
        
        if y &gt;= ystart+8
        {
            dir = -1;
        }
        if y &lt;= ystart-8
        {
            dir = 1;
        }
        if instance_exists(SnakeBoss)
        {
            if SnakeBoss.dead
            &amp;&amp; insideSection(SnakeBoss.x,SnakeBoss.y)
            {
                Shake = false;
                yspeed = 0;
            }
        }
    }
}
else if dead
{
    phase = 0;
    xspeed = 0;
    yspeed = 0;
    dir = startDir;
    y = ystart+yOffset;
}
