if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    preground = ground;
    xs = xspeed;
    
    checkGround();
    gravityCheckGround();
    generalCollision();
    
    if !preground &amp;&amp; ground
    {
        playSFX(sfxBikkyLand);
    }
    if xs != 0 &amp;&amp; xspeed = 0
    {
        instance_create(x, y, objHarmfulExplosion);
        playSFX(sfxExplosion2);
        dead = true;
        healthpoints = 0;
        event_user(10);
    }
    
    x += xspeed;
    y += yspeed;
    
    switch (phase)
    {
        case 0: //start facing mega man
            if instance_exists(target)
            {
                calibrateDirection();
                phase = 1;
                timer = 16;
            }
            break;
        case 1: //moving towards the player
            if timer
            {
                if ground
                {
                    timer -= 1;
                    if !timer {calibrateDirection();}
                }
            }
            else
            {
                if ground
                {
                    xspeed = 1.7 * image_xscale;
                }
                else
                {
                    xspeed = 0;
                    imgIndex = 0;
                    timer = 32;
                }
            }
            break;
    }
    
    if xspeed != 0
    {
        imgIndex += 0.25;
        if (imgIndex &gt;= 2)
        {
            imgIndex = imgIndex mod 2;
        }
    }
}
else if dead
{
    phase = 0;
    xspeed = 0;
    timer = 0;
}

image_index = imgIndex div 1;
</string>
