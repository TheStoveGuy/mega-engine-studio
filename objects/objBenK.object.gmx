if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    checkGround();
    gravityCheckGround();
    generalCollision();
    
    switch (phase){
        //start facing the player
        case 0:
            calibrateDirection();
            phase = 1;
            break;
        
        //rotating spear
        case 1:
            imgIndex += imgSpd;
            if (imgIndex &gt;= 6){
                imgIndex = 4 + imgIndex mod 6;
            }
            
            timer += 1;
            
            if (timer == 60){
                calibrateDirection(); //only turn a tiny bit before shooting
            }
            
            if (timer &gt;= 90){ //&lt;-- time spinning spear here
                phase = 2;
                imgIndex = 6;
                timer = 0;
                
                spear = instance_create(x + sprite_width * 0.65, y, objBenKSpear);
                spear.image_xscale = image_xscale;
            }
            
            break;
            
        //shoot spear
        case 2:
            if (imgIndex &lt; 7){
                imgIndex += imgSpd;
            }else{
                timer += 1;
                if (timer &gt;= 30){
                    phase = 3;
                    timer = 0;
                    imgIndex = 0;
                }
            }
            
            if (imgIndex &gt;= 8){
                imgIndex = 7;
            }
            
            break;
        
        //grab another spear
        case 3:
            if (imgIndex &lt; 2){
                imgIndex += imgSpd / 4;
            }else if (imgIndex &lt; 3){
                imgIndex += imgSpd / 2;
            }else{
                timer += 1;
                if (timer &gt;= 30){
                    phase = 1;
                    timer = 0;
                    imgIndex = 4;
                }
            }
            
            break;
    }
    
    x += xspeed;
    y += yspeed;
}
else
{
    if dead == true
    {
        phase = 0;
        timer = 0;
        xspeed = 0;
        yspeed = 0;
        imgIndex = 4;
    }
}

image_index = imgIndex div 1;
</string>
