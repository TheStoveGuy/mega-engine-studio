        <arguments>
          <argument>
            <kind>1</kind>
            <string>event_inherited();

if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    Timer += 1;
    switch(Active)
    {
        case 0: // Resting
            if Timer == 60
            {
                yspeed = 1;
            }
            if Timer == 61
            {
                yspeed = 0;
                if image_index == 1
                {
                    shootID = instance_create(bbox_left, y+14, objEnemyBullet);
                    shootID.xspeed = -2;
                    shootID = instance_create(bbox_right, y+14, objEnemyBullet);
                    shootID.xspeed = 2;
                }
            }
            if Timer == 120
            {
                yspeed = -1;
            }
            if Timer == 121
            {
                if image_index == 0
                {
                    image_index = 1;
                }
                else
                {
                    image_index = 0;
                }
                yspeed = 0;
                Timer = 0;
            }
            with objMegaman
            {
                if ground
                {
                    with other
                    {
                        if place_meeting(x,y-image_yscale,other.id) &amp;&amp; !place_meeting(x,y,other.id)
                        {
                            Active = 1;
                        }
                    }
                }
            }
            break;
        case 1: // Activate
            image_index = 2;
            if !place_meeting(x,y+YVel,objSolid) &amp;&amp; !place_meeting(x,y+YVel,objTopSolid)
            {
                if YVel &lt; 7 {YVel += 0.25;}
            }
            else {
                Active = 2
                yspeed = 0;
                YVel = 0;
                instance_create(x,y+23,objHoverSmoke)
            }
            yspeed = YVel;
            break;
        case 2: //Rise
            if place_meeting(x,y+3,objSolid) or place_meeting(x,y+3,objTopSolid)
            {
                yspeed = -1;
            }
            else {
                yspeed = 0;
                Active = 3;
            }
            image_index = 1;
            break;
        case 3: //Move
            xspeed = dir;
            if !place_meeting(x,y+5,objSolid) &amp;&amp; !place_meeting(x,y+5,objTopSolid)
            {
                Active = 4;
                xspeed = 0;
            }
            else
            {
                generalCollision();
                if xspeed = 0
                {
                    instance_create(x,y+16,objExplosion)
                    dead = true;
                }
            }
            break;
        case 4: //Fall
            if !place_meeting(x,y+YVel,objSolid) &amp;&amp; !place_meeting(x,y+YVel,objTopSolid)
            {
                if YVel &lt; 7 {YVel += 0.25;}
            }
            else
            {
                Active = 2
                yspeed = 0;
                YVel = 0;
            }
            yspeed = YVel;
            break;
    }
}
else if dead
{
    Active = 0;
    image_index = 0;
    YVel = 0;
    Timer = 0;
    x = xstart;
    y = ystart;
    xspeed = 0;
    yspeed = 0;
}
</string>
