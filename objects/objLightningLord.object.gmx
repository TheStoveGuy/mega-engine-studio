            <string>event_inherited();

if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    calibrateDirection()
    
    if !attachedToCloud
    {
        checkGround();
        gravityCheckGround();
        generalCollision();
    }
    else
    {
        x = instance_nearest(x,y,objMM2CloudPlatform).x;
        y = instance_nearest(x,y,objMM2CloudPlatform).y;
    }
    
    animEndme += 1;
    if animEndme &gt;= 8
    {
        if image_index == 0
        {
            image_index = 1;
        }
        else if image_index == 1
        {
            image_index = 0;
        }
        animEndme = 0;
    }
    
    shootTimer += 1;
    if shootTimer == 150
    {
        image_index = 2;
        a = instance_create(x,y-22,objLightningLordLightning);
        a.image_xscale = image_xscale;
    }
    if shootTimer == 180
    {
        image_index = 0;
        shootTimer = 0;
    }
    
    x += xspeed;
    y += yspeed; 
}
else if dead
{
    shootTimer = 0;
    animEndme = 0;
    image_index = 0;
}
</string>
