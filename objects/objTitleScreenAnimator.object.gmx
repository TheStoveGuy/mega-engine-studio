            <string>//skip intro animation
if (global.keyPausePressed[0])
{
    event_user(0);
    event_user(1);
}

if (phase == -1){
    playSFX(sfxSlashClaw);
    phase = 0;
}

switch (phase){
    //make a good mega man level whooshes on screen
    case 0:
        makeAGoodX += abs(makeAGoodX - makeAGoodFinalX) * 0.05 + 0.35;
        megaManLevelX -= (megaManLevelX - megaManLevelFinalX) * 0.05 + 0.35;
        
        if (makeAGoodX &gt; makeAGoodFinalX || megaManLevelX &lt; megaManLevelFinalX){
            phase = 1
            makeAGoodX = makeAGoodFinalX;
            megaManLevelX = megaManLevelFinalX;
        }
        break;
    
    //triangle piece materializes
    case 1:
        trianglePieceImgIndex += trianglePieceImgSpd;
        if (trianglePieceImgIndex &gt;= 29){
            phase = 2;
            flashAlpha = 1;
            event_user(0); //show final title screen &amp; starfield
            playSFX(sfxMenuSelect);
        }
        break;
    
    //flash
    case 2:
        if (flashAlpha &gt; 0){
            flashAlpha -= 0.012; //&lt;-- flashAlpha disappear speed
        }else{
            event_user(1); //spawn objTitleScreen and delete this
        }
    
        break;
}
</string>
