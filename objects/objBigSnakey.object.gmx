    {
        BodySolid = instance_create(x-16,y,objSolid);
        BodySolid.image_xscale = 2;
        BodySolid.image_yscale = length;
        BodySolid.depth = 1000;
    }
}
else
{
    if instance_exists(BodySolid)
    {
        with BodySolid instance_destroy();
    }
    if !insideView()
    {
        image_index = 0;
        shootTimer = 0;
    }
}
