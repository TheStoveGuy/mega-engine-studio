{
    if scroll &gt; 0
    {
        scroll -= 1;
    }
}
if global.keyDownPressed[0]
{
    if scroll+limit &lt; ds_list_size(ids)
    {
        scroll += 1;
    }
}

if keyboard_check_pressed(vk_shift) {
    ds_list_clear(objects);
    ds_list_clear(ids);
    ds_map_clear(names);
    ds_map_clear(amounts);
    
    with all {
        ds_list_add(other.objects,id);    
    }
    
    for (i=0; ds_list_size(objects)&gt;i; i+=1;)
    {
        obj = ds_list_find_value(objects,i);
        index = string(obj.object_index);
        if !ds_map_exists(names,index) { //Check if is not in the list
            ds_list_add(ids,index);
            n = object_get_name(real(index));
            n = string_delete(n,1,3);
            ds_map_add(names,index,n);
        }
        if !ds_map_exists(amounts,index) { //Add amounts of the object
            ds_map_add(amounts,index,1)
        }
        else {
            ds_map_replace(amounts,index,ds_map_find_value(amounts,index)+1)
        }
    }
    
    limit = 26;
    if ds_list_size(ids) &lt; limit {
        limit = ds_list_size(ids);
    }
    
    scroll = 0;
}

if keyboard_check_pressed(vk_backspace) {
    ds_list_clear(objects);
    ds_list_clear(ids);
    ds_map_clear(names);
    ds_map_clear(amounts);
    limit = 0;
    scroll = 0;
}

if keyboard_check_pressed(ord('P'))
{
    col += 1;
    if col &gt; 54 {col = 0;}
}
</string>
