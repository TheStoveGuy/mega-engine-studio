                    else {
                        if (xspeed &gt; -maxspeed) {
                            xspeed -= accel;
                        }
                    }
                }
            }
            if mode = 'forward' {
                if (image_xscale = 1) {
                    if (xspeed &lt; 1) {xspeed += accel;}
                }
                else {
                    if (xspeed &gt; -1) {xspeed -= accel;}
                }
            }
        }
        checkGround();
        gravityCheckGround();
        ys = yspeed;
        dir = image_xscale;
        generalCollisionTurnaround();
        if mode = 'chase' {
            if instance_exists(target) {
                image_xscale = dir;
            }
        }
    }
    x += xspeed;
    y += yspeed;
    if ys &gt; 0 &amp;&amp; yspeed = 0 &amp;&amp; ground {
        action = 2;
        yspeed = -ys*0.5;
        if yspeed &gt; -0.25 {yspeed = 0;}
    }
}
else if dead {
