        <arguments>
          <argument>
            <kind>1</kind>
            <string>if init
{
    init = 0;
    switch col {
        case 0: sprite_index = sprBigEyeRed; break;
        case 1: sprite_index = sprBigEyeBlue; break;
        case 2: sprite_index = sprBigEyeOrange; break;
        default: sprite_index = sprBigEyeRed; break;
    }
}

event_inherited();

if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper {
    checkGround();
    if ground {
        moveTimer += 1;
        if moveTimer == 1 {
            xs = 0;
            if repeatAmount &lt; 2 {
                randomize();
                highJump = choose(true, false); //False means a low jump, true means a high jump
            }
            else {
                highJump = !repeatIsHigh;
            }
            
            if !prevGround {
                playSFX(sfxHeavyLand);
            }
            image_index = 4;
                
            //To not make the Big Eye spam one jump height by pure randomness
            //Because if he spams low jumps, it's nearly impossible to pass him
            if highJump &amp;&amp; repeatIsHigh {
                repeatAmount += 1;
            }
            else if !highJump &amp;&amp; !repeatIsHigh {
                repeatAmount += 1;
            }
            else {
                repeatAmount = 1;
                repeatIsHigh = highJump;
            }
                
            calibrateDirection();
            
            xspeed = 0;
            yspeed = 0;
        }
        else if moveTimer == 4 {
            image_index = 0;
        }
        else if moveTimer == 6 {
            if highJump {
                image_index = 1;
            }
            else {
                image_index = 0;
            }
        }
        else if moveTimer == 40 {
            if highJump {
                yspeed = -6;
                xs = image_xscale * 1;
                image_index = 3;
            }
            else {
                yspeed = -3;
                xs = image_xscale * 1;
                image_index = 2;
            }
            moveTimer = 0;
        }
    }
    if xs != 0 {
        xspeed = xs;
    }
    
    checkGround();
    gravityCheckGround();
    generalCollision();
    
    x += xspeed;
    y += yspeed;
    
    prevGround = ground;
}
else if dead {
    moveTimer = 0;
    image_index = 2;
    repeatAmount = 0;
    repeatIsHigh = true;
    xs = 0;
}
</string>
