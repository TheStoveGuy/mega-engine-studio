if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    switch (phase)
    {
        case 0: //start facing Mega Man
            calibrateDirection();
            phase = 1;
            break;
        case 1: //wait to chase mega man
            if (instance_exists(target))
            {
                waitTimer += 1;
                if (waitTimer &gt;= 90)
                {
                    phase = 2;
                    waitTimer = 0;
                    imgIndex = 1;
                }
            }
            break;
        case 2: //look around once, and then chase mega man on the second look
            if (!animBack)
            {
                imgIndex += imgSpd;
                if (imgIndex &gt;= 7)
                {
                    imgIndex = 6 - imgIndex mod 7;
                    animBack = true;
                }
            }
            else
            {
                imgIndex -= imgSpd;
                if (imgIndex &lt; 1)
                {
                    if (!chase)
                    {
                        imgIndex = 2;
                        animBack = false;
                        chase = true;
                        speed = 1.5;
                        calibrateDirection();
                        
                        if (instance_exists(target))
                        {
                            direction = point_direction(x, y, target.x, target.y);
                        }
                    }
                    else
                    {
                        phase = 1;
                        chase = false;
                        speed = 0;
                        direction = 0;
                        animBack = false;
                        imgIndex = 0;
                    }
                }
            }
            break;
    }
    
    //animate propeller
    propellerTimer += 1;
    if (propellerTimer &gt;= 3) //&lt;-- propeller animation speed here
    {
        if (sprite_index == sprPropellerEyePropeller1)
        {
            sprite_index = sprPropellerEyePropeller2;
        }
        else
        {
            sprite_index = sprPropellerEyePropeller1;
        }
        
        propellerTimer = 0;
    }
}
else if dead
