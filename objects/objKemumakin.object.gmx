if global.frozen == false &amp;&amp; dead == false &amp;&amp; global.flashStopper == false
{
    if (moveTimer == -2)
    {
        col = choose(random_range(0, 26), random_range(27, 39), random_range(41, 53));
        moveTimer = 0;
        
        //starting direction
        if (instance_exists(target))
        {
            if (target.x &lt; x)
            {
                direction = 180;
            }
            else
            {
                direction = 0;
            }
            
            if (target.onSled)
            {
                if (instance_exists(objMegaman8Sled))
                {
                    var i;
                    for (i = 0; i &lt; instance_number(objMegaman8Sled); i += 1)
                    {
                        sled = instance_find(objMegaman8Sled, i);
                        if (sled.x &gt; view_xview &amp;&amp; sled.x &lt; view_xview + view_wview &amp;&amp; sled.y &gt; view_yview &amp;&amp; sled.y &lt; view_yview + view_hview)
                        {
                            xspeed = sled.MaxSpeed * sled.dir;
                        }
                    }
                }
            }
        }
    }
    
    if (moveTimer &lt;= 0)
    {
        directionSpd = random_range(-4, 4);
        moveTimer = random_range(10, 50);
    }
    else
    {
        moveTimer -= 1;
    }
    
    cloudTimer += 1;
    if (cloudTimer &gt;= 13)
    {
        cloudTimer = 0;
        cloud = instance_create(x, y, objKemumakinCloud);
        cloud.col = col;
    }
    
    speed = 1.6;
    direction += directionSpd;
    
    if (xspeed == 0)
    {
        if (direction &gt; 90 &amp;&amp; direction &lt; 270)
        {
            image_xscale = -1;
        }
        else
        {
            image_xscale = 1;
        }
    }
    else
    {
        if (xspeed &lt; 0)
        {
            image_xscale = -1;
        }
        else
        {
            image_xscale = 1;
        }
    }
    
    x += xspeed;
}
else if dead
{
    moveTimer = -2;
    cloudTimer = 0;
    col = 0;
    xspeed = 0;
    speed = 0;
    direction = 0;
}
