          <argument>
            <kind>1</kind>
            <string>event_inherited();

if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    switch phase
    {
        //Idle
        case 0:
            if topSolidID
            {
                with target
                {
                    if ground
                    {
                        if place_meeting(x,y+global.GravityDir,other.topSolidID)
                        &amp;&amp; !place_meeting(x,y,other.topSolidID)
                        {
                            with other
                            {
                                phase = 1;
                                timer = 0;
                                if image_index &lt;= 1 {iphase = 2;}
                                else {iphase = 3;}
                            }
                            break;
                        }
                    }
                }
            }
            break;
        //Preparing to drop down
        case 1:
            timer += 1;
            if timer &gt;= 12
            {
                timer = 0;
                phase = iphase;
            }
            break;
        //Dropping down animation
        case 2:
            timer += 1;
            if timer == 4 {
                image_index += 1;
                x+=2;
                if image_index = 1 {
                    image_index += 1;
                }
                if image_index == 2 {
                    issolid = 0;
                }
                if image_index == 5 {
                    phase = 0;
                    issolid = 1;
                }
                timer = 0;
            }
            break;
        case 3:
            timer += 1;
            if timer == 4 {
                image_index -= 1;
                x-=2;
                if image_index = 5 {
                    image_index -= 1;
                }
                if image_index == 4 {
                    issolid = 0;
                }
                if image_index == 1 {
                    phase = 0;
                    issolid = 1;
                }
                timer = 0;
            }
            break;
    }
    
    if image_index &gt;= 5 or image_index &lt;= 1
    {
        timeri += 1;
        if timeri &gt;= 15
        {
            if image_index = 0 or image_index = 5 {
                image_index += 1;
            }
            else if image_index = 1 or image_index = 6 {
                image_index -= 1;
            }
            timeri = 0;
        }
    }
    else {timeri = 0;}
}
else if dead
{
    issolid = 1;
    image_index = 0;
    canDrop = true;
    phase = 0;
    timer = 0;
    timeri = 0;
}

//Handle solid
if !topSolidID
{
    if issolid &amp;&amp; global.GravityDir = image_yscale &amp;&amp; !dead
    {
        topSolidID = instance_create(x-23*image_xscale, y, objTopSolid);
        if image_index &gt; 4 {topSolidID.x += 30*image_xscale;}
        if image_yscale = -1 {topSolidID.y -= 16;}
        //topSolidID.visible = 1;
    }
}

if topSolidID
{
    if !issolid or global.GravityDir != image_yscale or dead
    or image_index == 2 or image_index == 3 or image_index == 4
    {
        instance_activate_object(topSolidID);
        with topSolidID {instance_destroy();}
        topSolidID = -1;
    }
}
