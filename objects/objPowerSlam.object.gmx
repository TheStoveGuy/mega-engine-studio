            <string>event_inherited();

if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    checkGround();
    generalCollision();
    
    switch (phase)
    {
        case 0: //start facing mega man
            calibrateDirection();
            phase = 1;
            break;
        case 1: //wait until jetting up
            waitTime = 71; //&lt;-- wait time until jetting up
            
            if (timer &lt; waitTime - 30){
                imgIndex += imgSpd;
                if (imgIndex &gt;= 2){
                    imgIndex = imgIndex mod 2;
                }
            }else{
                imgIndex = 0;
                calibrateDirection(); //only turn when readying jump
            }
            
            if (instance_exists(target))
            {
                timer += 1;
                if (timer &gt;= waitTime)
                { 
                    phase = 2;
                    timer = 0;
                    yspeed = -2;
                    playSFX(sfxEnemyBoost);
                }
            }
            break;
        case 2: //moving towards the player
            imgIndex += imgSpd * 8;
            if (imgIndex &gt;= 3)
            {
                imgIndex = 1 + imgIndex mod 3;
            }
            
            down = false;
            
            //go down if a ceiling is hit, or the top of the screen is hit
            if (yspeed == 0)
            {
                down = true;
            }
            
            xspeed = 2 * image_xscale;
            
            //go down anyway if enough time passes
            timer += 1;
            if (timer &gt;= 60)
            {
                down = true;
            }
            
            if (instance_exists(target))
            {
                //go down if the top of the section is hit
                if (bbox_top &lt; global.sectionTop + 8){
                    down = true;
                }
                
                //go down if mega man is below
                if (target.bbox_left &gt; bbox_left - 4 &amp;&amp; target.bbox_right &lt; bbox_right + 4){
                    down = true;
                }
            }
            
            //go down
            if (timer &gt;= 15 &amp;&amp; down)
            {
                phase = 3;
                xspeed = 0;
                yspeed = 0;
                timer = 0;
                imgIndex = 1;
            }
            break;
        case 3: //slam down
            //wait until going down
            waitTime = 13;
            if (timer == waitTime)
            {
                yspeed = 6.1;
                imgIndex = 1;
                playSFX(sfxEnemyDrop2);
                timer += 1; //used timer as a boolean
            }
            else if (timer &lt; waitTime)
            {
                timer += 1;
            }
            
            if (ground)
            {
                phase = 1;
                timer = 0;
                audio_stop_sound(sfxEnemyDrop2);
                playSFX(sfxDachoneWalk); //that's actually the landing sfx in game. Not named the best in this engine
            }
            break;
    }
    
    x += xspeed;
    y += yspeed;
}
else if dead
{
    phase = 0;
    xspeed = 0;
    yspeed = 0;
    timer = 0;
    imgIndex = 0;
}

image_index = imgIndex div 1;
</string>
