if !global.frozen
{
    x+=xspeed;
    y+=yspeed;
    
    if type = 'jet'
    {
        with objRushTeleportBlock
        {
            if instance_exists(other.parent)
            {
                y = round(other.parent.y+14);
            }
        }
    }
    
    if yspeed != 0 &amp;&amp; upordown = 'down'
    {
        touchblock = 0;
        //objSolid
        if place_meeting(x,y,objSolid)
        {
            if type = 'coil' or type = 'sakugarne' {
                if !canland &amp;&amp; !touchblock {
                    touchblock = 1;
                }
                if canland &amp;&amp; !touchblock {
                    generalCollision();
                }
            }
            if type = 'jet' {
                if !canland &amp;&amp; !touchblock {
                    touchblock = 1;
                }
            }
        }
        //objTeleportBlock
        if place_meeting(x,y,objRushTeleportBlock)
        {
            if type = 'coil' or type = 'sakugarne' {
                if !touchblock &amp;&amp; !canneverland {
                    canland = 1;
                }
                else if touchblock &amp;&amp; !canneverland {
                    canneverland = 1;
                }
            }
            if type = 'jet' {
                if !touchblock &amp;&amp; !canneverland {
                    yspeed = 0;
                    y=instance_place(x,y,objRushTeleportBlock).y;
                }
                else if touchblock &amp;&amp; !canneverland {
                    canneverland = 1;
                }
            }
        }
    }
    else
    {
        if upordown = 'down'
        {
            if teleportTimer == 0 {
                image_index = 0;
                playSFX(sfxTeleportIn);
            }
            if teleportTimer == 3
                image_index = 1;
            else if teleportTimer == 6
                image_index = 0;
            else if teleportTimer == 9
                image_index = 2;
            else if teleportTimer = 13 {
                i = 0;
                global.playerProjectileCreator = parent;
                if type = 'jet' {
                    i = instance_create(x,y-16,objRushJet);
                }
                if type = 'coil' {
                    i = instance_create(x,y,objRushCoil);
                }
                if type = 'sakugarne' {
                    i = instance_create(x,y-16,objSakugarne);
                }
                if i
                {
                    i.image_xscale = dir;
                    i.parent = parent;
                }
                instance_destroy();
            }
        }
        else
        {
            if teleportTimer == 0 {
                image_index = 2;
                playSFX(sfxTeleportOut);
            }
            if teleportTimer == 3
                image_index = 0;
            else if teleportTimer == 6
                image_index = 1;
            else if teleportTimer == 9
                image_index = 0;
            else if teleportTimer = 13 {
                yspeed = -8;
            }
        }
        teleportTimer += 1;
    }
}
</string>
