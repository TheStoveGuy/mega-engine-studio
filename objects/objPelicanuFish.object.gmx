if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper {
    //le force of the pulling
    yspeed += grav;
    
    //so stremleaned
    checkGround();
    generalCollision();
    
    //anim nation wow
    if (!animBack){
        imgIndex += imgSpd;
        if (imgIndex &gt;= 3){
            imgIndex = 2 - imgIndex mod 3;
            animBack = true;
        }
    }else{
        imgIndex -= imgSpd;
        if (imgIndex &lt; 0){
            imgIndex = 1;
            animBack = false;
        }
    }
    
