if !global.frozen &amp;&amp; !dead &amp;&amp; !global.flashStopper
{
    //Set direction
    if !calibrated
    {
        if instance_exists(target) {
            if collision_point(bbox_get_xcenter(),bbox_top,objSolid,false,false) { //On the Ceiling
                horver = 'h'; dir = -sp;
                while place_meeting(x,y,objSolid) {y+=1;}
            }
            if collision_point(bbox_get_xcenter(),bbox_bottom,objSolid,false,false) { //On the Floor
                horver = 'h'; dir = sp;
                while place_meeting(x,y,objSolid) {y-=1;}
            }
            if collision_point(bbox_left,bbox_get_ycenter(),objSolid,false,false) { //On the Right
                horver = 'v'; dir = -sp;
                while place_meeting(x,y,objSolid) {x+=1;}
            }
            if collision_point(bbox_right,bbox_get_ycenter(),objSolid,false,false) { //On the Left
                horver = 'v'; dir = sp;
                while place_meeting(x,y,objSolid) {x-=1;}
            }
            if horver = 'h' {
                if target.x &gt; x {xspeed = sp;}
                else {xspeed = -sp;}
            }
            if horver = 'v' {
                if target.y &gt; y {yspeed = sp;}
                else {yspeed = -sp;}
            }
            calibrated = 1;
        }
    }
    if horver = 'h'
    {
        if !place_meeting(x,y+dir,objSolid)
        {
            yspeed = dir; horver = 'v'; y+=dir;
            dir = -xspeed; xspeed = 0;
        }
        else if place_meeting(x+xspeed,y,objSolid)
        {
            yspeed = -dir; horver = 'v'; y+=dir;
            dir = xspeed; xspeed = 0;
        }
    }
    else if horver = 'v'
    {
        if !place_meeting(x+dir,y,objSolid)
        {
            xspeed = dir; horver = 'h'; x+=dir;
            dir = -yspeed; yspeed = 0;
        }
        else if place_meeting(x,y+yspeed,objSolid)
        {
            xspeed = -dir; horver = 'h'; x+=dir;
            dir = yspeed; yspeed = 0;
        }
    }
    
    x+=xspeed;
    y+=yspeed;
}
else if dead
{
    calibrated = 0;
    xspeed = 0;
    yspeed = 0;
    image_index = 0;
}
</string>
