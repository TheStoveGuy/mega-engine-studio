            fallen = true;
        }
    }
    
    if xspeed == 0 &amp;&amp; fallen &amp;&amp; !crashed
    {
        playSFX(sfxTimeStopper);
        xspeed = 0;
        alarm[0] = 60;
        crashed = true
        fallen = false;
    } 
    else if fallen &amp;&amp; !crashed 
    {
        xspeed += 0.1*sign(dir);
    }
}
