            if boss = 1 { //Boss flash effect
                canHit = false;
                alarm[9] = 1;
                alarm[10] = max(1,HitInvun); //Being able to get hit again
                drawBoss = true;
                drawHitSpark = false;
            }
            else { //Regular enemy flash effect
                if canflash
                {
                    visible = false;
                    alarm[11] = 2;
                    flashing = 4;
                }
            }
        }
    }
}
if pierces = 0 {instance_destroy();}
</string>
